<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="595.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DeletePackage.DeleteView">
   <children>
      <AnchorPane fx:id="headline" prefHeight="91.0" prefWidth="850.0" style="-fx-background-color: #3D9999;">
         <children>
            <Button fx:id="b_back" layoutX="771.0" layoutY="30.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="72.0" style="-fx-background-color: #3d9950;" text="&lt;- Back" />
         </children></AnchorPane>
      <AnchorPane layoutY="91.0" prefHeight="503.0" prefWidth="850.0" style="-fx-background-color: #3D4956;">
         <children>
            <VBox layoutX="96.0" layoutY="117.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label prefHeight="45.0" prefWidth="104.0" text="User name:" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label prefHeight="45.0" prefWidth="104.0" text="Password:" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <TextField fx:id="userTxt" layoutX="196.0" layoutY="128.0" prefHeight="31.0" prefWidth="124.0" />
            <PasswordField fx:id="passTxt" layoutX="196.0" layoutY="173.0" prefHeight="31.0" prefWidth="124.0" />
            <Button fx:id="b_delete" layoutX="208.0" layoutY="243.0" mnemonicParsing="false" onAction="#deleteUser" prefHeight="45.0" prefWidth="100.0" text="Delete" />
            <Label fx:id="l_error" layoutX="397.0" layoutY="134.0" prefHeight="50.0" prefWidth="208.0" text="Wrong Username/Password" textAlignment="CENTER" textFill="#e42424" visible="false">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
